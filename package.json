{
  "name": "ts-grpc",
  "version": "1.0.0",
  "description": "gRPC with Node.js and TypeScript",
  "main": "index.js",
  "repository": "git@github.com:RodrigoMattosoSilveira/ts-grpc.git",
  "author": "Rodrigo Silveira <r3850365@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "run-s": "ts-node src/grpc/server/index.ts",
    "run-c-readOneCM": "ts-node src/grpc/client/index readOneCM",
    "run-c-readAllCM": "ts-node src/grpc/client/index  readAllCM",
    "run-c-createCM": "ts-node src/grpc/client/index  createCM",
    "run-c-updateCM": "ts-node src/grpc/client/index  updateCM",
    "run-c-deleteCM": "ts-node src/grpc/client/index  deleteCM",
    "run-c-createT": "ts-node src/grpc/client/index  createT",
    "run-c-readOneT": "ts-node src/grpc/client/index  readOneT",
    "proto": "sh src/grpc/proto/build.sh",
    "server": "ts-node src/grpc/server/index.ts",
    "client": "ts-node src/grpc/client/index.ts"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "google-protobuf": "^3.20.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "shortid": "^2.2.16",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@types/dotenv": "^8.2.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^18.0.1",
    "@types/shortid": "^0.0.29",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
