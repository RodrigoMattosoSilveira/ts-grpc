{
  "name": "ts-grpc",
  "version": "1.0.0",
  "description": "gRPC with Node.js and TypeScript",
  "main": "index.js",
  "repository": "git@github.com:RodrigoMattosoSilveira/ts-grpc.git",
  "author": "Rodrigo Silveira <r3850365@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "run-s-dev": "NODE_ENV=dev ts-node src/grpc/server/index.ts",
    "run-s-e2e": "NODE_ENV=e2e ts-node src/grpc/server/index.ts",
    "run-s-prd": "NODE_ENV=prd ts-node src/grpc/server/index.ts",
    "run-c-readOneCM": "NODE_ENV=dev && ts-node src/grpc/client/index readOneCM",
    "run-c-readAllCM": "NODE_ENV=dev && ts-node src/grpc/client/index  readAllCM",
    "run-c-createCM": "NODE_ENV=dev && ts-node src/grpc/client/index  createCM",
    "run-c-updateCM": "NODE_ENV=dev && ts-node src/grpc/client/index  updateCM",
    "run-c-deleteCM": "NODE_ENV=dev && ts-node src/grpc/client/index  deleteCM",
    "run-c-createT": "NODE_ENV=dev && ts-node src/grpc/client/index  createT",
    "run-c-readOneT": "NODE_ENV=dev && ts-node src/grpc/client/index  readOneT",
    "run-c-readAllT": "NODE_ENV=dev && ts-node src/grpc/client/index  readAllT",
    "run-c-updateT": "NODE_ENV=dev && ts-node src/grpc/client/index  updateT",
    "run-c-deleteT": "NODE_ENV=dev && ts-node src/grpc/client/index  deleteT",
    "test_readOneCM": "NODE_ENV=e2e && jest --runInBand --detectOpenHandles -t 'Read a Club Member'",
    "proto": "sh src/grpc/proto/build.sh",
    "server": "NODE_ENV=e2e ts-node src/grpc/server/index.ts",
    "client": "ts-node src/grpc/client/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "google-protobuf": "^3.20.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "shortid": "^2.2.16",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@types/dotenv": "^8.2.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.1",
    "@types/shortid": "^0.0.29",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
